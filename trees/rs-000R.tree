\title{topological space}
\date{2023-08-09}
\taxon{definition}
\author{rileyshahar}

\texpackage{preamble}
\texpackage{quiver}
\import{macros01}

\p{A \em{topological space} is a tuple #{(X, O)}, where #{O\subseteq \cP(X)} are
called the \em{open sets}, such that:}
\ul{
  \li{#{\emptyset\in O}, #{X\in O};}
  \li{#{O} is closed under arbitrary unions;}
  \li{#{O} is closed under finite intersections.}
}
